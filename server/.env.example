PORT=5000
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-name>.mongodb.net/fyxedwonen?retryWrites=true&w=majority
JWT_SECRET=your_jwt_secret_key_here_change_in_production

# Instructions:
# 1. Rename this file to .env
